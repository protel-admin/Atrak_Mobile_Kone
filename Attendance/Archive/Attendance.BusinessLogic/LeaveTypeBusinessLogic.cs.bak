using System.Linq;
using System.Text;
using System.Threading.Tasks;
using Attendance.Model;
using Attendance.Repository;

namespace Attendance.BusinessLogic
{
    public class LeaveTypeBusinessLogic
    {
        public List<LeaveView> GetAllLeaveTypes()
